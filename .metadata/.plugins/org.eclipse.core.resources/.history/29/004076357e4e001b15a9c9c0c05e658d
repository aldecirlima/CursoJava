import java.util.Scanner;

public class ExemploScannerQuebraPend {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s1, s2, s3;
		// Chamando o metodo sc.nextLine o sistema captura o texto
		// at√© que seja pressionado a tecla enter.
		s1 = sc.nextLine();
		s2 = sc.nextLine();
		s3 = sc.nextLine();
		System.out.println("DADOS DIGITADOS:");
		System.out.println(s1);
		System.out.println(s2);
		System.out.println(s3);
		sc.close();
	}
}